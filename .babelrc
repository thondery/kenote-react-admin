{ 
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "react-hot-loader/babel",
    "syntax-dynamic-import",
    "transform-decorators-legacy",
    "lodash", 
    ["import", [
      { 
        "libraryName": "antd", 
        "libraryDirectory": "lib",
        "style": "css" 
      }
    ]],
    [
      "module-resolver", {
        "alias": {
          "src": "./src"
        }
      }
    ]
  ],
  "compact": true
}